{
    "name": "yaodem/sendthen-php-sdk",
    "version": "1.1.2-alpha",
    "description": "SendThen REST API client for PHP",
    "type": "library",
    "keywords": [
        "yaodem",
        "sendthen",
        "sms",
        "api"
    ],
    "homepage": "https://github.com/yaodem/sendthen-php-sdk",
    "support": {
        "issues": "https://github.com/yaodem/sendthen-php-sdk/issues",
        "source": "https://github.com/yaodem/sendthen-php-sdk"
    },
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "^7.3",
        "ext-curl": "^7.4",
        "ext-json": "^7.4"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^5.3",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Denis AKPAGNONITE",
            "email": "akpagnonited@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SendThen\\": "src/SendThen/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}